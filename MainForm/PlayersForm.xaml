<Window x:Class="MainForm.PlayersForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MainForm"
        mc:Ignorable="d"
        Title="PlayersForm" Height="222" Width="314" WindowStartupLocation="CenterOwner" Icon="Player.png">
    <Grid>
        <StackPanel x:Name ="StackPlayers" Margin="0,0,156,1" RenderTransformOrigin="0.5,0.5">
            <TextBlock></TextBlock>
        </StackPanel>
        <TextBox x:Name="textboxPlayer" HorizontalAlignment="Left" Height="23" Margin="160,60,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button" Content="Добавить" HorizontalAlignment="Left" Margin="180,105,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Label x:Name="label" Content="Введите имя игрока" HorizontalAlignment="Left" Margin="160,19,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
